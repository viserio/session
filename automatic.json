{
    "configurators": {
        "env": {
            "SESSION_DRIVER": "file",
            "SESSION_LIFETIME": 120
        },
        "providers": {
            "Viserio\\Component\\Session\\Provider\\SessionServiceProvider": [
                "global"
            ]
        },
        "proxies": {
            "Viserio\\Component\\Session\\Proxy\\Session": [
                "global"
            ]
        },
        "options": {
            "viserio": {
                "session": {
                    "default": "%env:SESSION_DRIVER%",
                    "env": "%env:APP_ENV%",
                    "lifetime": "%env:SESSION_LIFETIME%",
                    "key_path": null,
                    "drivers": []
                }
            }
        }
    }
}
